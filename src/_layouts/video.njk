---
layout: main
---
<div class="video">
  <h1>{{ title }}</h1>

  {{ content | safe }}

  {% if youtube %}
    <iframe
      width="640"
      height="390"
      src="//www.youtube.com/embed/{{ youtube.id }}"
      frameborder="0"
      allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture"
      allowfullscreen
     ></iframe>
  {% endif %}
</div>
